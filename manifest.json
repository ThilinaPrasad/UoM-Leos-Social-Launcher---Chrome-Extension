{
  "manifest_version": 2,

  "name": "UoM Leos Social Launcher",
  "short name": "UoM Leos Social Launcher",
  "description": "Easy way to access UoM Leos Social Media",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
